#!/bin/bash
alias core_all=`pftt core_all`
alias core_named=`pftt core_named`
alias core_list=`pftt core_list`
alias app_all=`pftt app_all`
alias app_named=`pftt app_named`
alias app_list=`pftt app_list`
alias ui_all=`pftt ui_all`
alias ui_named=`pftt ui_named`
alias ui_list=`pftt ui_list`
alias release_get=`pftt release_get`
alias release_list=`pftt release_list`
alias rl=`release_list`
alias rg=`release_get`
alias rgn=`pftt rgn`
alias rgp=`pftt rgp`
alias aa=`app_all`
alias an=`app_named`
alias al=`app_list`
alias ua=`user_all`
alias un=`user_named`
alias ul=`user_list`
alias ca=`core_all`
alias cn=`core_named`
alias cl=`core_list`
echo
echo
echo
echo           PFTT Shell
echo
echo
echo Type `pftt ^<enter^>` to get help with PFTT. ^<tab^> for auto-completion. 
echo
echo Command Aliases:
echo core_all      core_named    core_list
echo app_all       all_named     app_list
echo ui_all        ui_named      ui_list
echo release_get   release_list  rl   rg   rgn   rgp
echo
echo
